<strong> Members Count: <%=@members.count %> members

<br>

<% @members.each do |member| %>
<section class="member">
</br>
</br>

  <section class = "name">
<strong> Name: <%=  member.name %></strong>
  <section class = "affiliation">
  <strong> Affiliation: <%= member.affiliation %> </strong>

  <section class="allies">
    <strong> Allies: <% member.allies.each do |ally| %>
    <%= ally %>
    <% end %> </strong>
  <section class="enemies">
    <strong> Enemies: <% member.enemies.each do |enemy| %>
    <%= enemy %>
  <% end %></strong>

  <section class="url">
    <% if member.url != nil %>
      <%= image_tag(member.url) %>
    <% end %>
  <% end %>
